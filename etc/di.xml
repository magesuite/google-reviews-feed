<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="MageSuite\GoogleReviewsFeed\Model\ReviewList">
        <arguments>
            <argument name="batchSize" xsi:type="string">5000</argument>
        </arguments>
    </type>

    <type name="\MageSuite\GoogleReviewsFeed\Model\ChildrenIdsProvider">
        <arguments>
            <argument name="childrenIdsProvider" xsi:type="array">
                <item name="bundle" xsi:type="object">MageSuite\GoogleReviewsFeed\Model\ChildrenIdsProvider\Bundle</item>
                <item name="configurable" xsi:type="object">MageSuite\GoogleReviewsFeed\Model\ChildrenIdsProvider\Configurable</item>
                <item name="grouped" xsi:type="object">MageSuite\GoogleReviewsFeed\Model\ChildrenIdsProvider\Grouped</item>
            </argument>
        </arguments>
    </type>
</config>
